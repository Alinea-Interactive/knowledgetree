<h3>Most Viewed Documents</h3>

<div style="overflow:hidden; width:230px;">
<table>
    <tbody>
        {foreach from=$data item=document}
        <tr>
            <td width="20">{$context->getMimeIcon($document.mime_id)}</td>
            <td class="docnowrap"><a href="{$context->cleanUrl($document.document_id)}">{$document.filename}</a></td>
        </tr>
        {/foreach}
    </tbody>
</table>
</div>